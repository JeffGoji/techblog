
{{#if posts.length}}
<div class="row center">
<section class="col-lg-6 col-md-6 col-sm-12">
  <h2 class="text-center">Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}


</section>
</div>
{{/if}}




<a href="/dashboard/new"><button type="submit" class="btn btn-primary m-3">Create a New Post</button></a>




<script src="/javascript/add-post.js"></script>
